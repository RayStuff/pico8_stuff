pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
name = "cassie"
x = 64 - #name * 4/2
y = 64 - 3
iter = 0

stepsize = 40
textcol = 2
boxcol = 7

function rnd_dir()
  return flr(rnd(4))
end

srand( time)
direction = rnd_dir()


function incr_max(v, max_val)
  return (v + 1) % max_val
end

function decr_max(v, reset_val)
  if (v == 0) return reset_val
  return v - 1
end

function _update()

  iter += 1

  if (iter % stepsize == 0) then
    direction = rnd_dir()
    --col = incr_max(col, 32)
  end
  
  if (direction == 3) then
    y+=1
    if (y > 127 - 5) then
      direction = 2
      sfx(0)
    end
  elseif (direction == 1) then
    x+=1
    if (x>127-#name*4) then 
      direction = 0
      sfx(0)
    end
  elseif (direction == 2) then 
    y-=1
    if (y < 1) then
      direction = 3
      sfx(0)
    end
  elseif (direction == 0) then
    x-=1
    if (x < 1) then
      direction = 1
      sfx(0)
    end
  end

  if (btn(0)) then 
    direction = 0
    iter = 0
  elseif (btn(1)) then
    direction = 1
    iter = 0
  elseif (btn(2)) then
    direction = 2
    iter = 0
  elseif (btn(3)) then
    direction = 3
    iter = 0
  end

  if (btnp(4)) then
    textcol = (textcol + 1) % 16 + 1
  end
  if (btnp(5)) then
    boxcol = (boxcol + 1) % 16 + 1
    -- avoid same color for both
			 if (boxcol == textcol) boxcol = (boxcol + 1) % 16 + 1

  end

end

function _draw()
  rectfill(0,0,127,127,0)
  rectfill(x-1,y-1,x+#name*4,y+6,boxcol)
  print(name, x, y, textcol)
end
