pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
tb = {}
function draw_textbox(x,y)
  rectfill(x,y,x+tb.width, y+tb.height, tb.fillcol)
  print(tb.name, x+1,y+1, tb.textcol)
end
-- tb = textbox
tb.name = "cassie"
tb.width = #tb.name * 4 + 1
tb.height = 6
tb.fillcol = 7
tb.textcol = 2
tb.draw = draw_textbox


function draw_cassie_spr(x,y)
  if (direction==1) then 
    spr(iter%4/2+1,x,y)
  elseif (direction==0) then 
    spr(iter%4/2+3,x,y)
  elseif (direction==2) then 
    spr(iter%4/2+5,x,y)
  else -- (direction==3) 
    spr(iter%4/2+7,x,y)
  end
end
cassie_spr = {}
cassie_spr.height = 8
cassie_spr.width = 8
cassie_spr.draw = draw_cassie_spr


sprite = cassie_spr

x = 64 - sprite.width/2
y = 64 - sprite.height/2
iter = 0

stepsize = 40

function rnd_dir()
  return flr(rnd(4))
end

srand( time)
direction = rnd_dir()


function incr_max(v, max_val)
  return (v + 1) % max_val
end

function decr_max(v, reset_val)
  if (v == 0) return reset_val
  return v - 1
end

function _update()

  iter += 1

  if (iter % stepsize == 0) then
    direction = rnd_dir()
    --col = incr_max(col, 32)
  end
  
  if (direction == 3) then
    y+=1
    if (y >= 127 - sprite.height) then
      direction = 2
      sfx(0)
    end
  elseif (direction == 1) then
    x+=1
    if (x >= 127 - sprite.width) then 
      direction = 0
      sfx(0)
    end
  elseif (direction == 2) then 
    y-=1
    if (y < 1) then
      direction = 3
      sfx(0)
    end
  elseif (direction == 0) then
    x-=1
    if (x < 1) then
      direction = 1
      sfx(0)
    end
  end

  if (btn(0)) then 
    direction = 0
    iter = 0
  elseif (btn(1)) then
    direction = 1
    iter = 0
  elseif (btn(2)) then
    direction = 2
    iter = 0
  elseif (btn(3)) then
    direction = 3
    iter = 0
  end

  if (btnp(4)) then
    if (sprite == tb) then
      sprite = cassie_spr
    else
      sprite = tb
    end
  end
--  if (btnp(5)) then
--    tb.fillcol = (tb.fillcol + 1) % 16 + 1
--    -- avoid same color for both
--			 if (tb.fillcol == tb.textcol) tb.fillcol = (tb.fillcol + 1) % 16 + 1
--  end

end

function _draw()
  rectfill(0,0,127,127,1)
--  rectfill(x-1,y-1,x+#name*4,y+6,boxcol)
--  print(name, x, y, textcol)
--  spr(1,x,y)
  sprite.draw(x,y)
  coord_str = "x=" .. x .. " y=" .. y
--  print(coord_str, 0, 0, 7) 
end
